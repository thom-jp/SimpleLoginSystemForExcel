VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "LoginSheet"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Public Property Get id() As String
    id = Range("C7").Value
End Property
Public Property Let id(id_ As String)
    Range("C7").Value = id_
End Property
Public Property Get Password() As String
    Password = Range("C9").Value
End Property
Public Property Let Password(pw_ As String)
    Range("C9").Value = pw_
End Property

